{
  "name": "@enjine/session",
  "version": "0.0.1-alpha.15",
  "description": "Session Manager",
  "author": "equaliser0 <accounts@equaliser.minimalcity.de>",
  "homepage": "https://github.com/AwesomeEcosystem/enjine#readme",
  "license": "MIT",
  "main": "dist",
  "types": "dist",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "npm run build:esm && npm run build:cjs && npm run build:umd",
    "build:esm": "tsc --module es2015 --target es6 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es6 --outDir dist/cjs",
    "build:umd": "tsc --module umd --target es6 --outDir dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AwesomeEcosystem/enjine.git"
  },
  "bugs": {
    "url": "https://github.com/AwesomeEcosystem/enjine/issues"
  },
  "dependencies": {
    "async-constructor": "^0.4.5",
    "axios": "^0.21.1",
    "socket.io-client": "^3.0.5"
  }
}
