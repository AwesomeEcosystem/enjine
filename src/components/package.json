    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "tailwindcss-dark-mode": "^1.1.7",
    "ts-node": "^8.10.2",
    "typescript": "~4.1.5",
